          <h1 class="course-name">スコア・ラウンドメモ編集画面</h1>
        <% else %>
          <h1 class="error-message"><%= @error %></h1>
        <% end %>
      </div>
      <%= render partial: "score_form", locals:{score:@score}%>
    </div>
  </div>